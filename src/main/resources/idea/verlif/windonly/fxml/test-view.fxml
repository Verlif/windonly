<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.ListView?>
<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="idea.verlif.windonly.WindonlyController">
    <top>
        <TextField fx:id="input" onMousePressed="#inputClicked" prefWidth="180.0"
                   BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="10.0"/>
            </BorderPane.margin>
        </TextField>
    </top>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <center>
        <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="200.0" prefWidth="200.0"
                    BorderPane.alignment="CENTER">
            <ListView fx:id="list" prefHeight="145.0" prefWidth="179.0"/>
        </ScrollPane>
    </center>
</BorderPane>
